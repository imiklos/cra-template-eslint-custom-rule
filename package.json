{
  "version": "0.1.2",
  "license": "MIT",
  "name": "eslint-plugin-custom-rule",
  "author": "Istvan Miklos",
  "main": "dist/index.js",
  "module": "dist/eslint-plugin-custom-rule.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**",
    "src/**"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "compile": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/eslint-plugin-custom-rule.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/eslint-plugin-custom-rule.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/eslint": "^8.4.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@typescript-eslint/utils": "^5.10.1",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
